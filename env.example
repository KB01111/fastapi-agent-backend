# FastAPI Agent Backend Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
DEBUG=false
LOG_LEVEL=info
HOST=0.0.0.0
PORT=8000

# Authentication (Clerk)
# Get these from your Clerk dashboard
CLERK_SECRET_KEY=sk_test_your_secret_key_here
CLERK_PUBLISHABLE_KEY=pk_test_your_publishable_key_here
JWT_ALGORITHM=RS256

# Database (Supabase)
# Get these from your Supabase project settings
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
DATABASE_URL=postgresql://postgres:password@db.your-project.supabase.co:5432/postgres

# AI Configuration
# Get these from your AI provider dashboards
OPENAI_API_KEY=sk-your-openai-api-key-here
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here

# CORS Configuration (JSON array format)
CORS_ORIGINS=["https://your-domain.com","https://www.your-domain.com","http://localhost:3000","http://localhost:19006"]

# Observability
PROMETHEUS_PORT=8001

# Gunicorn Configuration
GUNICORN_WORKERS=4

# Grafana Configuration
GRAFANA_PASSWORD=your-secure-grafana-password 